{%
set classes = [
'block',
'block-' ~ configuration.provider|clean_class,
'block-' ~ plugin_id|clean_class,
]

%}
<div{{ attributes.addClass(classes) }}>
    {{ title_prefix }}
<div>
{% if content.field_header_title %}
<div class="row">
    <div class="col-md-12">
        <h3 class="h1 heading-primary mt-xl">{{ content.field_header_title | render | striptags | trim}}</h3>
    </div>
</div>
{% endif %}

<div class="row">
    <div class="col-md-12">
        <ul class="history">
            {{ content.field_our_history }}
        </ul>

    </div>
</div>

</div>
    {{ title_suffix }}
</div>